public static String CompressString(String s)
    {    String s1 ="";
          String s2 = "";
        int count =1;
        for(int i=0;i<s.length();i++)
        {
            if((i+1) < s.length() && s.charAt(i) == s.charAt(i+1))
            {
                count++;
                
            }else{
                
                s2 = s2 + s.charAt(i);
                s2 +=count;
                s1 =s2;
                count =1;
            }
        }
        return s1;
    }
