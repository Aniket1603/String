public static String  sortCharacter(String s)
    
     {    
         char ch[] = s.toCharArray();
        for(int i=0;i<s.length();i++)
        {
            for(int j=i+1;j<s.length();j++)
            {
                if(ch[i] > ch[j])
                {
                    char temp  =ch[i];
                    ch[i] = ch[j];
                    ch[j] =temp;
                }
                
            }
        }
        String s1 = new String(ch);
        return s1;
        
    }
